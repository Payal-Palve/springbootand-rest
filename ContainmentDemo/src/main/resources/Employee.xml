<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

<bean id="employee" class="com.pojo.Employee">
<property name="empid" value="102"></property>
<property name="name"  value="akash"></property>
<property name="salary" value="9000"></property>
<property name="date" ref="dateobj"></property>
</bean>

<bean id="dateobj" class="com.pojo.Date">
<property name="dd" value="10"></property>
<property name="mm"  value="11"></property>
<property name="yy" value="2024"></property>
</bean>
</beans>